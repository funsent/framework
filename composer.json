{
    "name": "funsent/framework",
    "type": "framework",
    "description": "the web application framework by PHP",
    "homepage": "http://www.funsent.com/",
    "keywords": [
        "php",
        "framework",
        "funsent"
    ],
    "license": "MIT",
    "authors": [{
        "name": "yanggf",
        "email": "2018708@qq.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "psr/log": "~1.0",
        "psr/container": "~1.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "files": [],
        "classmap": [],
        "psr-4": {
            "funsent\\": "framework/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "funsent\\tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}